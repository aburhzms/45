int main() {
	vector <int> atz{ 3, 8, 7, 4 };
	vector <string> name{ "Anna", "Antons", "Vlads", "Artemijs" };
	int i = 0;

	int abc;
	cin >> abc;
	do {
		if (atz[i] == abc) cout << name[i];
		i++;
	} while (i < atz.size());
	atz.pop_back();
	name.pop_back();
	name.at(0) = "Antons";
	name.push_back("Alla");
	atz. push_back(6);
	i = 0;
	do {
		cout << atz[i] << endl;
		cout << name[i] << endl;
		i++;
	} while (i < atz.size() && name.size());
}
